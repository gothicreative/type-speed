{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/dist"
      }
    },
    {
      "src": "server/api/login.js",
      "use": "@vercel/node"
    },
    {
      "src": "server/api/register.js",
      "use": "@vercel/node"
    },
    {
      "src": "server/api/results.js",
      "use": "@vercel/node"
    },
    {
      "src": "server/api/users.js",
      "use": "@vercel/node"
    },
    {
      "src": "server/api/leaderboard.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/login",
      "dest": "server/login.js"
    },
    {
      "src": "/register",
      "dest": "server/register.js"
    },
    {
      "src": "/results",
      "dest": "server/results.js"
    },
    {
      "src": "/users/(.*)",
      "dest": "server/users.js"
    },
    {
      "src": "/leaderboard",
      "dest": "server/leaderboard.js"
    },
    {
      "src": "/(.*)",
      "dest": "client/dist/index.html"
    }
  ]
}